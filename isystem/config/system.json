{
  "Name":"ISystem II",
  "Platform":"x86",
  "Version":"1.0",
  "Group":"Ihsoh Software",
  "WriteLogToDiskInRealTime":true,
  "EnableAPIC":false,
  "UseRTCForTaskScheduler":false,
  "RTCRate":15,
  "Timezone":"UTC+08:00",
  "IFS1":{
    "EnableReadBuffer":true,
    "EnableAppendBuffer":true
  },
  "Console":{
    "ShowVerInfoWhenBoot":true,
    "ShowDiskInfoWhenBoot":true,
    "ShowMemoryInfoWhenBoot":true,
    "ShowCPUInfoWhenBoot":true,
    "ShowClock":true,
    "PromptWhenDirIsNotSelected":"#NO DIR> ",
    "PromptColorWhenDirIsNotSelected":4,
    "PromptWhenDirIsSelected":"$%s> ",
    "PromptColorWhenDirIsSelected":2
  }
}
